(:name bbdb
       :website "http://bbdb.sourceforge.net/"
       :description "The Insidious Big Brother Database (BBDB) is a contact management utility."
       :type git
       :url "git://github.com/emacsmirror/bbdb.git"
       :load-path ("./lisp")
       :build `("autoconf" ,(concat "./configure --with-emacs=" el-get-emacs) "make autoloadsc" "make")
       :features bbdb
       :autoloads nil
       :post-init (lambda () (bbdb-initialize)))
